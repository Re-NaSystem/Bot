{
  "name": "rena_bot",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "up": "npm-run-all build start",
    "build": "tsc -p . && tsc-alias",
    "start": "node --trace-warnings dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint ./src/* --ignore-path .prettierignore --max-warnings 0",
    "lint:typecheck": "tsc --pretty --noEmit",
    "format": "run-s format:eslint format:prettier",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discord-player/extractor": "^4.4.4",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^2.0.1",
    "@distube/ytdl-core": "^4.13.2",
    "axios": "^1.5.0",
    "chalk": "^4.1.2",
    "discord-player": "^6.6.4",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "ffmpeg-static": "^5.2.0",
    "fs": "^0.0.1-security",
    "glob": "^7.2.0",
    "i18n": "^0.15.1",
    "mongoose": "^7.5.1",
    "play-dl": "^1.9.7",
    "tsc-alias": "^1.8.8",
    "youtube-ext": "^1.1.14",
    "yt-stream": "^1.4.8",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.2",
    "@types/glob": "^8.1.0",
    "@types/i18n": "^0.13.6",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsc-paths-resolver": "^0.1.3",
    "typescript": "^5.2.2"
  }
